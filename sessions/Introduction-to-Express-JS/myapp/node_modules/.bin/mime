#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/mime@1.6.0/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/mime/cli.js/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Express-JS/myapp/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else 
  exec node  "$basedir/../mime/cli.js" "$@"
fi
